      crossorigin="anonymous"
    />
    <title>Profilpage</title>
    
    
    <script>
      
    </script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css" integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l" crossorigin="anonymous">
    <link rel="stylesheet" href="../public/dashboard.css" />
</head>

<body>


    <form id="change-form" method="POST" action="/change/:username" enctype="multipart/form-data">
        <div class="container">
        <h1>Update User</h1>
        <div class="form-group col-md-5">
        <label for="username">username</label>
        <input
        name = "username"
        id="username"
        type="text"
        autocomplete="off"
        value= <%= currentUser %>
        class="form-control"
        />
        </div>
        <div class="form-group col-md-5">
        <label for="email">username</label>
        <input
        name = "email"
        id="email"
        type="email"
        autocomplete="off"
        value= <%= currentEmail %>
        class="form-control"
        />
        </div>
        <div class="form-group col-md-5">
        <label for = "picupload">Choose picture</label>
        <input
        type="file" 
        name="picupload"
        class="form-control-file" 
        />
        </div>
        <button type="submit" value="Upload" class="btn btn-primary">Submit</button><br> <br>
        <ul>
            <li><a href="/dashboard">Dashboard</a></li>
        </ul>
            
        </div>
    </form>


</body>
</html>